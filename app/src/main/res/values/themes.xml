<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CoachMe" parent="Theme.MaterialComponents.DayNight.DarkActionBar">


        <!-- Removing Action Bar -->
        <item name="windowNoTitle">true</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/cm_purple</item>
        <item name="colorPrimaryVariant">@color/cm_purple2</item>
        <item name="colorOnPrimary">@color/cm_colorOnPrimary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cm_colorOnSecondary</item>
        <item name="colorSecondaryVariant">@color/cm_colorOnPrimary</item>
        <item name="colorOnSecondary">@color/cm_colorOnPrimary</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>
    <style name="CalendarDateGrayedOut">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="appointmentCardView">
        <item name="cardBackgroundColor">#FFF9f9f9</item>
        <item name="cardCornerRadius">10dp</item>
        <item name="cardElevation">8dp</item>
        <item name="cardUseCompatPadding">false</item>
        <item name="contentPadding">4dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#2C0066</item>
        <item name="android:strokeColor">#6B66C7</item>
    </style>
    <style name="filledButtonCM" parent="Widget.Material3.Button.ElevatedButton.Icon">
      <item name="android:backgroundTint">#6B66C7</item>
      <item name="android:textColor">#fffaff</item>
        <item name="android:elevation">4dp</item>
    </style>



</resources>